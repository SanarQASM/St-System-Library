<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-opacity: 0.7;" styleClass="switchAnchor" stylesheets="@application.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.settingController">
   <children>
