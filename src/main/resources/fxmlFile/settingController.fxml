            <Font name="Rockwell Condensed Bold" size="15.0" />
         </font>
         <styleClass>
            <String fx:value="bothForm" />
            <String fx:value="allButton" />
            <String fx:value="bothFormButton" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button alignment="CENTER" layoutX="108.0" layoutY="186.0" mnemonicParsing="false" onAction="#changeEmail" prefHeight="29.0" prefWidth="132.0" stylesheets="@application.css" text="Change Email" textAlignment="CENTER" textFill="#00b4d8" wrapText="true">
