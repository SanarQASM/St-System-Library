      </Label>
      <Button alignment="CENTER" layoutX="108.0" layoutY="51.0" mnemonicParsing="false" onAction="#changeUsername" prefHeight="29.0" prefWidth="132.0" stylesheets="@application.css" text="Change Username" textAlignment="CENTER" textFill="#00b4d8" wrapText="true">
         <font>
            <Font name="Rockwell Condensed Bold" size="15.0" />
         </font>
         <styleClass>
            <String fx:value="bothForm" />
            <String fx:value="allButton" />
            <String fx:value="bothFormButton" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="49.0" fitWidth="46.0" layoutX="36.0" layoutY="43.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/register.png" />
