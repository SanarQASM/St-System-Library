            <Font name="Rockwell Condensed Bold" size="15.0" />
         </font>
         <styleClass>
            <String fx:value="bothForm" />
            <String fx:value="allButton" />
            <String fx:value="bothFormButton" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="49.0" fitWidth="46.0" layoutX="38.0" layoutY="110.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/forgot-password.png" />
         </image>
      </ImageView>
      <Button alignment="CENTER" layoutX="113.0" layoutY="360.0" mnemonicParsing="false" onAction="#backToMainController" prefHeight="29.0" prefWidth="78.0" stylesheets="@application.css" text="Back" textAlignment="CENTER" textFill="#00b4d8" wrapText="true">
